{	
	"twitter": {
		"consumer_key": {
			"regex": "^[0-9a-zA-Z]{18,25}$"
		},
		"consumer_secret":{
			"regex": "^[0-9a-zA-Z]{38,50}$"
		},
		"access_token": {
			"regex": "^[0-9]*-[0-9a-zA-Z]{38,45}$"
		},
		"access_token_secret":{
			"regex": "^[0-9a-zA-Z]{38,45}$"
		},
	},
	"facebook": {
		"client_id": {
			"regex": "^[0-9]{15}$",
		},
		"client_secret": {
			"regex": "^[0-9a-zA-Z]{32}$",
		},
	},
}